@model Wardship.Models.Report

@{
    ViewBag.Title = "Index";
}

@using (Html.BeginForm("Index", "Report", FormMethod.Post, new { autocomplete = "off" }))
{
    @Html.ValidationSummary(true)
    @Html.AntiForgeryToken()

    <fieldset style="width:500px" >
        <legend>Records by date range report</legend>

   
        <div class="editor-label">    
            @Html.LabelFor(model => model.ReportBegin)</div>
        <div class="editor-field">
            @Html.EditorFor(model => model.ReportBegin)
        </div>

        
        <div class="editor-label">    
            @Html.LabelFor(model => model.ReportFinal)</div>
        <div class="editor-field">
            @Html.EditorFor(model => model.ReportFinal)
        </div>

        <p>
            <input type="submit" value="Run Report" />
        </p>

    </fieldset>
    
}
