  </configSections>
  <connectionStrings>
    <!--<add name="DataContext" connectionString="Data Source=(localdb)\ProjectsV12;Integrated Security=SSPI;Initial Catalog=Wardship;MultipleActiveResultSets=True" providerName="System.Data.SqlClient" />-->
    <add name="DataContext" connectionString="Server=localhost;port=5432;database=Wardship;user id=postgres;password=pwd" providerName="Npgsql" />
    <add name="xls" connectionString="Provider=Microsoft.Jet.OLEDB.4.0;Data Source={0};Extended Properties='Excel 8.0;HDR=Yes;IMEX=1;ImportMixedTypes=Text'" />
  </connectionStrings>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <add key="pageSize" value="15" />
    <add key="RouteDebugger:Enabled" value="false" />
    <add key="passwordExpiresInDays" value="90" />
    <add key="ida:AADInstance" value="https://login.microsoftonline.com/" />
    <add key="ida:Domain" value="justiceuk.onmicrosoft.com" />
    <add key="ida:TenantId" value="c6874728-71e6-41fe-a9e1-2e8c36776ad8" />
    <add key="CurServer" value="Unknown" />
    <add key="enableSimpleMembership" value="false" />
    <add key="DataContextName" value="DataContext" />
  </appSettings>
  <!--
    For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.6.1" />
      </system.Web>
